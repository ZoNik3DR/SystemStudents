<?xml version="1.0" encoding="utf-8"?>
<configuration>
   <configSections>
       <section name="PicsSection" type="ManagementSystemStudents.HelpClasses.PicsSection, ManagementSystemStudents"/>
   </configSections> 
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7"/>
    </startup>
   

  <appSettings>
    <add key="SaveM" value="Ctrl+S"/>
    <add key="OpenM" value="Ctrl+L"/>
    <add key="ExitM" value="Esc"/>

    <add key="PluginsFolder" value="\plugins\"/>
  </appSettings>

   
   <PicsSection>
     <PicsCollection>
         <Pic target="SaveM" path="\Resources\saveicon.png"/>     
         <Pic target="OpenM" path="\Resources\openicon.png"/>     
         <Pic target="ExitM" path="\Resources\exit_icon.png"/>    
         <Pic target="MainIcon" path="\Resources\icon.ico"/>     

         <Pic target="RightPlus" path="\Resources\right-arrow.png"/>   
         <Pic target="LeftPlus" path="\Resources\left-arrow1.png"/>     

         <Pic target="LeftArrow" path="\Resources\left-arrow-2.png"/>   
         <Pic target="LeftArrows" path="\Resources\left-arrow-1.png"/>     
         <Pic target="RightArrow" path="\Resources\right-arrow-2.png"/>   
         <Pic target="RightArrows" path="\Resources\right-arrow-1.png"/>     

     </PicsCollection>
   </PicsSection>
    


</configuration>
